    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">
    <!--
        Added the constraints for entity ProductRating.
    -->
    <changeSet id="20211026231019-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="product_rating_id"
                                 baseTableName="product_rating"
                                 constraintName="fk_product_rating__product_rating_id"
